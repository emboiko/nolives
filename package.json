{
  "name": "livetowin",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/app.js",
    "server": "env-cmd -f ./config/dev.env nodemon ./src/app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "emboiko",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.3.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.10",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-bnet": "^2.0.0"
  }
}
